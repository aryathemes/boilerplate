{
    "name": "aryathemes/boilerplate",
    "description": "Boilerplate for new PHP projects.",
    "type": "project",
    "keywords": ["boilerplate"],
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\": "test"
        }
    }
}
