{
    "name": "aryathemes/boilerplate",
    "description": "Boilerplate for new PHP projects.",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["library", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "name@example.com",
            "homepage": "https://example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --bootstrap vendor/autoload.php"
        ]
    }
}
